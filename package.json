{
  "name": "cpe_project",
  "version": "1.0.0-beta.2",
  "description": "A Class President Election Program",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "deploy": "electron-builder --windows nsis:ia32"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bukgeuk/CPE_Project.git"
  },
  "author": "Bukgeuk_, Sechack06",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bukgeuk/CPE_Project/issues"
  },
  "homepage": "https://github.com/Bukgeuk/CPE_Project#readme",
  "dependencies": {
    "fs-extra": "^9.0.1",
    "html2canvas": "^1.0.0-rc.7",
    "jquery": "^3.5.1",
    "toastr": "^2.1.4"
  },
  "build": {
    "productName": "학급 정부회장 선거",
    "appId": "com.cpe.app",
    "asar": true,
    "protocols": {
      "name": "CPE_Project",
      "schemes": [
        "cpe_project"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./assets/icons/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": true
    },
    "extraFiles": ["version.json"],
    "directories": {
      "buildResources": "./version.json",
      "output": "./dist/",
      "app": "."
    }
  },
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-is-dev": "^1.2.0"
  }
}
